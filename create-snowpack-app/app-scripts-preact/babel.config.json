{
  "presets": [
    [
      "@babel/preset-react",
      {
        "pragma": "h",
        "pragmaFrag": "Fragment"
      }
    ],
    [
      "@babel/preset-typescript",
      {
        "jsxPragma": "h"
      }
    ]
  ],
  "plugins": ["@babel/plugin-syntax-import-meta"],
  "env": {
    "development": {
      "plugins": ["react-refresh/babel"]
    }
  }
}
