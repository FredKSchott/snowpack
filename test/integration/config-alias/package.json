{
  "private": true,
  "version": "1.0.0",
  "name": "@snowpack/test-config-alias",
  "description": "Handle deep imports to package entrypoints",
  "scripts": {
    "testinstall": "snowpack"
  },
  "snowpack": {
    "installOptions": {
