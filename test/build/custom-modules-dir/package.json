{
  "private": true,
  "version": "1.0.0",
  "name": "@snowpack/test-custom-modules-dir",
  "description": "This test ensures that Snowpack can build when /my_modules is the web modules dir",
  "scripts": {
    "testbuild": "snowpack build"
  },
  "snowpack": {
    "webModulesUrl": "/my_modules",
    "mount": {
      "./src": "/_dist_"
    },
    "buildOptions": {
      "minify": false
    }
  },
  "dependencies": {
    "array-flatten": "^3.0.0"
  },
  "devDependencies": {
    "snowpack": "*"
  }
}
