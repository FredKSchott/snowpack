{
  "private": true,
  "version": "1.0.0",
  "name": "@snowpack/test-custom-modules-dir",
  "description": "This test ensures that Snowpack can build when /my_modules is the web modules dir",
  "scripts": {
    "testbuild": "snowpack build"
  },
  "snowpack": {
    "scripts": {
      "mount:src": "mount src --to /_dist_",
      "mount:web_modules": "mount $WEB_MODULES --to /my_modules"
    },
    "buildOptions": {
      "minify": false
    }
  },
  "dependencies": {
    "array-flatten": "^3.0.0"
  },
  "devDependencies": {
    "snowpack": "*"
  }
}
