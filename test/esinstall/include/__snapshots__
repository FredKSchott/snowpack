function extend (to, _from) {
  for (const key in _from) {
    to[key] = _from[key];
  }
  return to
}
/**
 * Merge an Array of Objects into a single Object.
 */
function toObject (arr) {
  const res = {};
  for (let i = 0; i < arr.length; i++) {
    if (arr[i]) {
      extend(res, arr[i]);
    }
  }
  return res
}
