{
  "name": "@pika/web",
  "version": "0.3.0",
  "description": "Install npm packages that run natively in the browser.",
  "author": "Fred K. Schott <fkschott@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pikapkg/pack.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
