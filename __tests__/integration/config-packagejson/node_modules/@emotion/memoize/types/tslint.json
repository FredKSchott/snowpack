{
    "extends": "dtslint/dtslint.json",
    "rules": {
        "array-type": [
            true,
            "generic"
        ],
        "import-spacing": false,
        "semicolon": false,
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-module",
            "check-rest-spread",
            "check-type",
            "check-typecast",
            "check-type-operator",
            "check-preblock"
        ],

        "no-unnecessary-generics": false
    }
}
