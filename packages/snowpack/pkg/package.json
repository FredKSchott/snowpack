    "p-queue": "^6.2.1",
    "resolve-from": "^5.0.0",
    "rimraf": "^3.0.0",
    "rollup": "^2.13.1",
    "rollup-plugin-esbuild": "^2.3.0",
    "signal-exit": "^3.0.3",
    "strip-comments": "^2.0.1",
    "tar": "^6.0.1",
    "validate-npm-package-name": "^3.0.0",
    "ws": "^7.3.0",
    "yargs-parser": "^18.1.3"
